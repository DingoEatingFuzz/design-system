{{#if @withJavascript}}
  {{#each this.elements as |e|}}
    <Hds::Dropdown::Togglewithjs
      @toggle={{e.toggle}}
      @text={{e.text}}
      @size={{e.size}}
      @color={{e.color}}
      @avatarUrl={{e.avatarUrl}}
    />
  {{/each}}
{{else}}
  {{#each this.elements as |e|}}
    <Hds::Dropdown::Toggle
      @toggle={{e.toggle}}
      @text={{e.text}}
      @size={{e.size}}
      @color={{e.color}}
      @avatarUrl={{e.avatarUrl}}
    />
  {{/each}}
{{/if}}